<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>
    <typeAliases>
        <!-- Entities 参数实体 -->
        <!-- com.comverse.timesheet.web.bean 包中的 所有Bean, 取个别名 -->
        <!-- 比如TestTable类就用Test来表示 -->
        <typeAlias type="com.user.vo.User" alias="User"/>
        <typeAlias type="com.exam.vo.Exam" alias="Exam"/>
        <typeAlias type="com.code.vo.Code" alias="Code"/>
        <typeAlias type="com.measure.vo.Measure" alias="Measure"/>
        <typeAlias type="com.performance.vo.Performance" alias="Performance"/>
    </typeAliases>
    <mappers>
        <!-- 对应Bean类的xml配置文件的路径信息 -->
        <mapper resource="com/user/vo/User.xml"/>
        <mapper resource="com/exam/vo/Exam.xml"/>
        <mapper resource="com/code/vo/Code.xml"/>
        <mapper resource="com/measure/vo/Measure.xml"/>
        <mapper resource="com/performance/vo/Performance.xml"/>
    </mappers>
</configuration>
